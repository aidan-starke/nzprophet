{
  "name": "prophet",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.16.0",
    "yarn": "1.22.10"
  },
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "src": "npm start --prefix src",
    "dev": "concurrently \"npm run server\" \"npm run src\"",
    "postinstall": "knex migrate:rollback && knex migrate:latest && knex seed:run"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^0.21.18",
    "nodemon": "^2.0.7",
    "path": "^0.12.7",
    "pg": "^8.5.1",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-scripts": "4.0.2",
    "redux": "^4.0.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint:recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^7.19.0",
    "sqlite3": "^5.0.2",
    "yarn-run-all": "^3.1.1"
  },
  "proxy": "http://localhost:5000/"
}
